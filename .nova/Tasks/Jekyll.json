{
  "actions" : {
    "build" : {
      "enabled" : true,
      "script" : "bundle"
    },
    "clean" : {
      "enabled" : true,
      "script" : "bundle exec jekyll clean"
    },
    "run" : {
      "enabled" : true,
      "script" : "bundle exec jekyll serve --livereload"
    }
  },
  "buildBeforeRunning" : true
}
